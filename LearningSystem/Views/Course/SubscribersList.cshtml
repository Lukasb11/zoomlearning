@model IEnumerable<LearningSystem.Models.user>

@{
    ViewBag.Title = "SubscribersList";
}

<h2>List of subscribers for selected course</h2>

<table class="table">
    <tr>
        <th>
            First name
        </th>
        <th>
            Last name
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.first_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.last_name)
            </td>
        </tr>
    }
</table>
@if (Model.Count() == 0)
{
    <H2>
        No courses found!
    </H2>
}